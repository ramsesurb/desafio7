

  <h1 class="text-danger text-center">Add Item</h1>

  <form  onsubmit= "return addItem(this);">

  <div class="form-group">
    <label for="title"><b>Title</b></label>
    <input type="text" placeholder="Fender American Stratocaster" id="title" name="title" class="form-control">
  </div>
  

  <div class="form-group">
    <label for="price"><b>Price</b></label>
    <input type="number" placeholder="899.99" id="price" name="price" class="form-control">
  </div>

  <div class="form-group">
    <label for="thumbnail"><b>thumbnail</label>
    <input type="text" placeholder="https://muzikercdn.com/uploads/products/789/78991/main_9a486311.jpg" id="thumbnail" name="thumbnail" class="form-control">
    <button  type="submit" class="btn btn-danger mt-3">Send</button>
  </div>

</form>


